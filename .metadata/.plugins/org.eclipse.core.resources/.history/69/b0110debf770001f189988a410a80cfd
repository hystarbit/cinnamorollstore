<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="OrderReturnCompleteDAO">
	<insert id="insertOrderReturnComplete" parameterType="OrderReturn">
		insert into
		order_return(application_number, order_item_number, 
		return_type,select_reason,content,delivery_price)
		values((select nvl(max(application_number), 50000000) + 1 from order_return),
		 #{order_item_number}, #{return_type}, #{select_reason},
		 #{content}, #{delivery_price})
	</insert>
</mapper>