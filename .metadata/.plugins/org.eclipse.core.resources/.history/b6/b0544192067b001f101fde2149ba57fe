package com.cinnamoroll.store.reply.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.cinnamoroll.store.reply.ReplyService2;
import com.cinnamoroll.store.reply.ReplyVO2;

@Service
public class ReplyServiceImpl2 implements ReplyService2 {
	
	@Autowired
	private ReplyDAO2 replyDAO2;
	
	@Override
	public ReplyVO2 getReply(ReplyVO2 vo) {
		// TODO Auto-generated method stub
		return replyDAO2.getReply(vo);
	}

	@Override
	public ReplyVO2 getRecentReply(ReplyVO2 vo) {
		// TODO Auto-generated method stub
		return replyDAO2.getRecentReply(vo);
	}

	@Override
	public List<ReplyVO2> getReplyList(ReplyVO2 vo) {
		// TODO Auto-generated method stub
		return replyDAO2.getReplyList(vo);
	}

	@Override
	public List<ReplyVO2> getReplyListPage(ReplyVO2 vo) {
		// TODO Auto-generated method stub
		return replyDAO2.getReplyListPage(vo);
	}

	@Override
	public int getReplyCount(ReplyVO2 vo) {
		// TODO Auto-generated method stub
		return replyDAO2.getReplyCount(vo);
	}

}
